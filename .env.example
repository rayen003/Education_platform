# Environment variables for Edtech_project
# Copy this file to .env and fill in your values

# OpenAI API key (required for LLM-based features)
OPENAI_API_KEY=your_openai_api_key_here

# Default LLM model to use
LLM_MODEL=gpt-4o-mini

# Optional: Port for the Streamlit app (default is 8501)
# STREAMLIT_PORT=8501

# Optional: Configure logging level (INFO, DEBUG, WARNING, ERROR)
# LOG_LEVEL=INFO 